{
  "name": "@vads/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "dev": "docusaurus start --port 3000",
    "build": "node scripts/set-cms-url.js prod && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "clean": "rm -rf .turbo node_modules .docusaurus build",
    "cms": "decap-server",
    "cms:url:dev": "node scripts/set-cms-url.js dev",
    "cms:url:prod": "node scripts/set-cms-url.js prod",
    "cms:generate-partials": "node scripts/generate-partials-config.js",
    "dev:cms": "node scripts/set-cms-url.js dev && node scripts/generate-partials-config.js && concurrently \"npm run dev\" \"npm run cms\""
  },
  "dependencies": {
    "@department-of-veterans-affairs/component-library": "^47.0.0",
    "@department-of-veterans-affairs/css-library": "^0.28.0",
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/theme-mermaid": "^3.9.2",
    "docusaurus-lunr-search": "^3.5.0",
    "lunr": "^2.3.9",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "concurrently": "^9.2.1",
    "decap-server": "^3.4.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
